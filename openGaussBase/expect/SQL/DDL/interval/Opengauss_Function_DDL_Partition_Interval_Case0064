-- @testpoint: interval分区,INSERT数据,间隔为1day,对边界值进行测试
drop table if exists table1;
SQL SUCCESS
create table table1( 
col_1 smallint,
col_2 char(30),
col_3 int,
col_4 timestamp without time zone not null,
col_5 boolean,
col_6 nchar(30),
col_7 float
)
partition by range (col_4)
interval ('1 day')
(
	partition table1_p1 values less than ('2020-01-01')
);
SQL SUCCESS
insert into table1 values (1,'aaa',1,'2019-12-31 23:59:59',true,'aaa',1.1);
SQL SUCCESS
insert into table1 values (2,'bbb',2,'2020-01-01 00:00:00',false,'bbb',2.2);
SQL SUCCESS
insert into table1 values (4,'ddd',4,'2020-01-01 23:59:59',false,'ddd',4.4);
SQL SUCCESS
insert into table1 values (7,'eee',7,'2020-01-02 23:59:59',true,'eee',5.5);
SQL SUCCESS
insert into table1 values (8,'eee',8,'2020-01-09 00:00:00',true,'eee',5.5);
SQL SUCCESS
insert into table1 values (6,'fff',6,'2020-01-05 12:00:00',false,'fff',6.6);
SQL SUCCESS
insert into table1 values (3,'ccc',3,'2020-01-03 00:00:01',true,'ccc',3.3);
SQL SUCCESS
select relname, parttype, partstrategy, boundaries, reltablespace, intervaltablespace, interval, transit
from pg_partition where parentid = (select oid from pg_class where relname = 'table1') order by relname;
+-----------+----------+--------------+-----------------------+---------------+--------------------+----------+---------+
| relname   | parttype | partstrategy | boundaries            | reltablespace | intervaltablespace | interval | transit |
+-----------+----------+--------------+-----------------------+---------------+--------------------+----------+---------+
| sys_p1    | p        | i            | {2020-01-02 00:00:00} | 0             |                    |          |         |
| sys_p2    | p        | i            | {2020-01-03 00:00:00} | 0             |                    |          |         |
| sys_p3    | p        | i            | {2020-01-10 00:00:00} | 0             |                    |          |         |
| sys_p4    | p        | i            | {2020-01-06 00:00:00} | 0             |                    |          |         |
| sys_p5    | p        | i            | {2020-01-04 00:00:00} | 0             |                    |          |         |
| table1    | r        | i            |                       | 0             |                    | {1 day}  |         |
| table1_p1 | p        | r            | {2020-01-01}          | 0             |                    |          |         |
+-----------+----------+--------------+-----------------------+---------------+--------------------+----------+---------+
select * from table1 partition (table1_p1)order by col_4;
+-------+--------------------------------+-------+-------------------------------+-------+--------------------------------+-------+
| col_1 | col_2                          | col_3 | col_4                         | col_5 | col_6                          | col_7 |
+-------+--------------------------------+-------+-------------------------------+-------+--------------------------------+-------+
| 1     | aaa                            | 1     | 2019-12-31 23:59:59.000000000 | true  | aaa                            | 1.1   |
+-------+--------------------------------+-------+-------------------------------+-------+--------------------------------+-------+
select * from table1 partition (sys_p1)order by col_4;
+-------+--------------------------------+-------+-------------------------------+-------+--------------------------------+-------+
| col_1 | col_2                          | col_3 | col_4                         | col_5 | col_6                          | col_7 |
+-------+--------------------------------+-------+-------------------------------+-------+--------------------------------+-------+
| 2     | bbb                            | 2     | 2020-01-01 00:00:00.000000000 | false | bbb                            | 2.2   |
| 4     | ddd                            | 4     | 2020-01-01 23:59:59.000000000 | false | ddd                            | 4.4   |
+-------+--------------------------------+-------+-------------------------------+-------+--------------------------------+-------+
select * from table1 partition (sys_p2)order by col_4;
+-------+--------------------------------+-------+-------------------------------+-------+--------------------------------+-------+
| col_1 | col_2                          | col_3 | col_4                         | col_5 | col_6                          | col_7 |
+-------+--------------------------------+-------+-------------------------------+-------+--------------------------------+-------+
| 7     | eee                            | 7     | 2020-01-02 23:59:59.000000000 | true  | eee                            | 5.5   |
+-------+--------------------------------+-------+-------------------------------+-------+--------------------------------+-------+
select * from table1 partition (sys_p3)order by col_4;
+-------+--------------------------------+-------+-------------------------------+-------+--------------------------------+-------+
| col_1 | col_2                          | col_3 | col_4                         | col_5 | col_6                          | col_7 |
+-------+--------------------------------+-------+-------------------------------+-------+--------------------------------+-------+
| 8     | eee                            | 8     | 2020-01-09 00:00:00.000000000 | true  | eee                            | 5.5   |
+-------+--------------------------------+-------+-------------------------------+-------+--------------------------------+-------+
select * from table1 partition (sys_p4)order by col_4;
+-------+--------------------------------+-------+-------------------------------+-------+--------------------------------+-------+
| col_1 | col_2                          | col_3 | col_4                         | col_5 | col_6                          | col_7 |
+-------+--------------------------------+-------+-------------------------------+-------+--------------------------------+-------+
| 6     | fff                            | 6     | 2020-01-05 12:00:00.000000000 | false | fff                            | 6.6   |
+-------+--------------------------------+-------+-------------------------------+-------+--------------------------------+-------+
select * from table1 partition (sys_p5)order by col_4;
+-------+--------------------------------+-------+-------------------------------+-------+--------------------------------+-------+
| col_1 | col_2                          | col_3 | col_4                         | col_5 | col_6                          | col_7 |
+-------+--------------------------------+-------+-------------------------------+-------+--------------------------------+-------+
| 3     | ccc                            | 3     | 2020-01-03 00:00:01.000000000 | true  | ccc                            | 3.3   |
+-------+--------------------------------+-------+-------------------------------+-------+--------------------------------+-------+
update table1 set col_4='2019-12-31 23:59:59' where col_1=6;
SQL SUCCESS
update table1 set col_4='2020-01-01 00:00:00' where col_1=7;
SQL SUCCESS
update table1 set col_4='2020-01-09 23:59:59' where col_1=8;
SQL SUCCESS
update table1 set col_4='2020-01-09 00:00:00' where col_1=4;
SQL SUCCESS
