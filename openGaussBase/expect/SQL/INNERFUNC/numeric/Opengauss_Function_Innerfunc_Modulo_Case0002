-- @testpoint: 数字操作符%(求余),浮点型数据相余
drop table if exists data_01;
SQL SUCCESS
create table data_01 (clo1 FLOAT(3),clo2 FLOAT4);
SQL SUCCESS
insert into data_01 values (255.00001, 32767.1234587);
SQL SUCCESS
select * from data_01;
+-----------+-----------+
| clo1      | clo2      |
+-----------+-----------+
| 255.00002 | 32767.123 |
+-----------+-----------+
select clo1 % clo2 from data_01;
+----------+
| ?column? |
+----------+
| 255.0    |
+----------+
SELECT 12.235648798 % 1.2145566333333333333344565633333333333344112222222222222222222245555555555555555556789 AS RESULT;
+-----------------------------------------------------------------------------------------+
| result                                                                                  |
+-----------------------------------------------------------------------------------------+
| 0.0900824646666666666554343666666666666558877777777777777777777544444444444444444432110 |
+-----------------------------------------------------------------------------------------+
SELECT 12 % 1.2145566333333333333344565633333333333344112222222222222222222245555555555555555556789 AS RESULT;
+-----------------------------------------------------------------------------------------+
| result                                                                                  |
+-----------------------------------------------------------------------------------------+
| 1.0689902999999999999898909299999999999902989999999999999999999789999999999999999988899 |
+-----------------------------------------------------------------------------------------+
drop table if exists data_01;
SQL SUCCESS
