-- @testpoint: power函数,参数为有效数值
select power(2,8) as result from sys_dummy;
+--------+
| result |
+--------+
| 256.0  |
+--------+
select power(2.0,8.0) as result from sys_dummy;
+----------------------+
| result               |
+----------------------+
| 256.0000000000000000 |
+----------------------+
