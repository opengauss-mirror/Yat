-- @testpoint: hll_hash_bytea(bytea, int32),对bytea类型数据计算哈希值，并设置hashseed（即改变哈希策略）
select hll_hash_bytea('DEADBEEF', 13);
+----------------------+
| hll_hash_bytea       |
+----------------------+
| -5318877104906997434 |
+----------------------+
select hll_hash_bytea('a', 16);
+----------------------+
| hll_hash_bytea       |
+----------------------+
| -5061344319391232414 |
+----------------------+
select hll_hash_bytea('@##$', 15);
+---------------------+
| hll_hash_bytea      |
+---------------------+
| 4350921890349700822 |
+---------------------+
select hll_hash_bytea('你好', 155);
+----------------------+
| hll_hash_bytea       |
+----------------------+
| -7567665588615764665 |
+----------------------+
