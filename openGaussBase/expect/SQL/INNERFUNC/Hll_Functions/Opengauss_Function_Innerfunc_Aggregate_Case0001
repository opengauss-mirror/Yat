-- @testpoint: hll_add_agg(hll_hashval) 描述：把哈希后的数据按照分组放到hll中
create table t_id(id int);
SQL SUCCESS
insert into t_id values(generate_series(1,10));
SQL SUCCESS
select  hll_add_agg(hll_hash_text(id)) from t_id ;
+----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------+
| hll_add_agg                                                                                                                                                                                                                |
+----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------+
| \x484c4c50000002002b09000000000000002440562207b7639d9cbdb9e9eea221d6e41c935482ae6bdba53a612603da299b80426dc2cfc61bed5156912708defd4191631369b4198fffa69b1aec8ad360e512a2a5bf62a0b65e6bab562207b7639d9cbde790965c97d5dcbe00 |
+----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------+
drop table t_id;
SQL SUCCESS
select hll_add_agg(hll_hash_boolean(true));
+----------------------------------------------------------------------------+
| hll_add_agg                                                                |
+----------------------------------------------------------------------------+
| \x484c4c08000002002b0900000000000000f03f83933dd2475001f183933dd2475001f100 |
+----------------------------------------------------------------------------+
select hll_add_agg (hll_hash_smallint(32767, 0));
+----------------------------------------------------------------------------+
| hll_add_agg                                                                |
+----------------------------------------------------------------------------+
| \x484c4c08000002002b0900000000000000f03fe814afa9cc0b7e7de814afa9cc0b7e7d00 |
+----------------------------------------------------------------------------+
