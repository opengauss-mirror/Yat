-- @testpoint: 输入参数为表达式
SELECT sqrt(2*8%3+17-2) AS RESULT;
+--------+
| result |
+--------+
| 4.0    |
+--------+
SELECT sqrt(100e+2) AS RESULT;
+-------------------+
| result            |
+-------------------+
| 100.0000000000000 |
+-------------------+
SELECT sqrt(sqrt(64)) AS RESULT;
+--------------------+
| result             |
+--------------------+
| 2.8284271247461903 |
+--------------------+
SELECT sqrt(power(64,2)) AS RESULT;
+--------+
| result |
+--------+
| 64.0   |
+--------+
