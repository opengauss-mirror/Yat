-- @testpoint: 数值函数嵌套的输入测试
select least(ACOS(-0.9),34) from sys_dummy;
+--------------------+
| least              |
+--------------------+
| 2.6905658417935308 |
+--------------------+
select least(COS(120 * 3.14159265359/180),34) from sys_dummy;
+---------------------+
| least               |
+---------------------+
| -0.5000000000001195 |
+---------------------+
select least(EXP(3.9),34) from sys_dummy;
+-------+
| least |
+-------+
| 34    |
+-------+
